pipeline {
    agent any

    stages {
        stage('Downloading the code') {
            steps {
                echo 'Downloading the code ......'
            }
        }
        stage('Build') {
            steps {
                echo 'Building...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}
